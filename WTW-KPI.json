{
	"name": "KPITemplate",
      "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"kpi": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"background": [{
					"show": true,
					"transparency": 0
				}],
				"border": [{
					"show": false,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"alignment": "center"
				}],
				"indicator": [{
					"showIcon": true,
					"horizontalAlignment": "left",
					"verticalAlignment": "top"
				}],
				"trendline": [{
					"show": true
				}],
				"goals": [{
					"showGoal": true,
					"showDistance": true
					}]
			}
		}
	}
}