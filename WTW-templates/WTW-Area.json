{
	"name": "WTWAreaTemplate",
    "dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"areaChart": {
            "*": {
                "general": [
                    {
                        "WTWcomment": "Test file comments",
                        "responsive": true,
                        "keepLayerOrder": true
                    }
                ],
                "legend": [
                    {
                        "show": true,
                        "position": "Top",
                        "showTitle": true,
                        "titleText": "WTW area chart legend",
                        "legendMarkerRendering": "markerOnly",
                        "matchLineColor": false,
                        "bold": true,
                        "titleBold": true
                    }
                ],
                "categoryAxis": [
                    {
                        "show": true,
                        "axisType": "categorical",
                        "concatenateLabels": true,
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW area chart category axis"
                    }
                ],
                "valueAxis": [
                    {
                        "show": true,
                        "position": "Right",
                        "axisScale": "Linear",
                        "labelDisplayUnits": "0",
                        "labelPrecision": "Auto",
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW area chart value axis"
                    }
                ],
                "dataPoint": [
                    {}
                ],
                "labels": [
                    {
                        "show": true,
                        "bold": true,
                        "titleBold": true,
                        "color": {
                            "solid": {
                                "color": "#2f2c31"
                            }
                        },
                        "enableBackground": true,
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backgroundTransparency": 0
                    }
                ],
                "lineStyles": [
                    {
                        "strokeWidth": 4,
                        "strokeLineJoin": "bevel",
                        "lineStyle": "solid",
                        "showMarker": true,
                        "markerShape": "square",
                        "markerSize": 10
                    }
                ],
                "plotArea": [
                    {}
                ]
            }
        }
	}
}