{
	"name": "HundredPercentStackedBarTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
        "hundredPercentStackedBarChart": {
            "*": {
                "general": [
                    {
                        "WTWcomment": "Test file comments",
                        "responsive": true,
                        "keepLayerOrder": true
                    }
                ],
                "legend": [
                    {
                        "show": true,
                        "position": "Top",
                        "showTitle": true,
                        "titleText": "WTW 100% Stacked Bar Chart Legend"
                    }
                ],
                "categoryAxis": [
                    {
                        "show": true,
                        "position": "Right",
                        "concatenateLabels": false,
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW 100% Stacked Bar Chart Legend"
                    }
                ],
                "valueAxis": [
                    {
                        "show": true,
                        "position": "Right",
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW 100% Stacked Bar Chart Value Axis"
                    }
                ],
                "dataPoint": [
                    {}
                ],
                "labels": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#2f2c31"
                            }
                        },
                        "labelOrientation": "horizontal",
                        "labelPosition": "OutsideEnd",
                        "enableBackground": true,
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backgroundTransparency": 0
                    }
                ],
                "plotArea": [
                    {
                        "transparency": 0
                    }
                ]
            }
        }
	}
}