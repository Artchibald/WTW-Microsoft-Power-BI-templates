{
	"name": "StackedBarTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
        "barChart": {
            "*": {
                "general": [
                    {
                        "WTWcomment": "Test file comments",
                        "responsive": true,
                        "keepLayerOrder": true
                    }
                ],
                "legend": [
                    {
                        "show": true,
                        "position": "Top",
                        "showTitle": true,
                        "titleText": "WTW stacked bar chart legend"
                    }
                ],
                "categoryAxis": [
                    {
                        "show": true,
                        "position": "Right",
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW stacked bar chart cat axis"
                    }
                ],
                "valueAxis": [
                    {
                        "show": true,
                        "axisScale": "Linear",
                        "labelDisplayUnits": "0",
                        "labelPrecision": "Auto",
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW stacked bar chart value axis"
                    }
                ],
                "labels": [
                    {
                        "show": true,
                        "labelDisplayUnits": "0",
                        "labelOrientation": "horizontal",
                        "enableBackground": true
                    }
                ]
            }
        }
	}
}