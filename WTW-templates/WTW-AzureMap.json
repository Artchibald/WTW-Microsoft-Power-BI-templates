{
	"name": "WTWAzureMapTemplate",
	"dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"azureMap": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true 
				}],
				"background": [{
					"show": true
				}],
				"border": [{
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"alignment": "center"
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"legendColor": { "solid": { "color": "#ffffff"}},
					"showTitle": true,
					"titleText": "WTW Azure Map info goes here"
				}]
            }
        }
    }
}