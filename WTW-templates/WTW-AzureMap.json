{
	"name": "WTWAzureMapTemplate",
	"dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
        "azureMap": {
            "*": {
                "general": [
                    {
                        "WTWcomment": "Test file comments",
                        "responsive": true,
                        "keepLayerOrder": true
                    }
                ],
                "background": [
                    {
                        "show": true
                    }
                ],
                "border": [
                    {
                        "show": false,
                        "radius": 0
                    }
                ],
                "title": [
                    {
                        "show": true,
                        "alignment": "left"
                    }
                ],
                "legend": [
                    {
                        "show": true,
                        "position": "Top",
                        "legendColor": {
                            "solid": {
                                "color": "#2f2c31"
                            }
                        },
                        "backgroundColor": { "solid": { "color": "#FFFFFF"}},
                        "showTitle": true,
                        "titleText": "WTW Azure Map info goes here"
                    }
                ],
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#2f2c31"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"enableBackground": true,
					"backgroundColor": { "solid": { "color": "#FFFFFF"}},
					"backgroundTransparency": 0
				}]
            }
        }
    }
}