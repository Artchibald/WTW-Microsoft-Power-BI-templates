{
	"name": "TableTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"tableEx": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"title": [{
					"show": true,
					"alignment": "left"
				}],
				"grid": [{
					"gridVertical": true,
					"gridVerticalWeight": 1,
					"gridHorizontal": true,
					"gridHorizontalWeight": 1,
					"rowPadding": 1,
					"outlineWeight": 1,
					"imageHeight": 100,
                    "outlineColor": { "solid": { "color": "#48086f"}}
				}],
				"columnHeaders": [{
					"outline": "Frame",
					"autoSizeColumnWidth": true,
					"alignment": "Left",
					"wordWrap": true
				}],
				"values": [{
					"outline": "Frame",
					"urlIcon": true,
					"wordWrap": true
				}],
				"total": [{
					"totals": true,
					"outline": "Frame"
				}],
				"stylePreset": [{ 
					"name": "BoldHeader"
				}]
			}
		}
	}
}