{
    "name": "WTW-theme3",
    "dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc",
        "#ffa3c2",
        "#ffbfac",
        "#007d61",
        "#b1232d"
    ],
    "firstLevelElements": "#2f2c31",
    "secondLevelElements": "#2f2c31",
    "thirdLevelElements": "#2f2c31",
    "fourthLevelElements": "#2f2c31",
    "background": "#FFFFFF",
    "foreground": "#3adcc9",
    "secondaryBackground": "#e2e2e2",
    "tableAccent": "#327fef",
    "good": "#007d61",
    "neutral": "#ffb92a", 
    "bad": "#b1232d",
    "maximum": "#327fef",
    "center": "#48086f",
    "minimum": "#c900ac",
    "null": "#f6517f",
    "textClasses": {
        "callout": {
            "fontSize": 40, 
            "fontFace": "Segoe UI",
            "color": "#2f2c31"
        },
        "title": {
            "fontSize": 36,
            "fontFace": "Segoe UI",
            "color": "#2f2c31"
        },
        "header": {
            "fontSize": 14,
            "fontFace": "Segoe UI", 
            "color": "#2f2c31"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Segoe UI",
            "color": "#2f2c31" 
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "responsive": true,
                        "wordWrap": true,
                        "fontSize": 11
                    }
                ],
                "wordWrap": [
                    {
                        "show": true
                    }
                ],
                "labels": [
                    {
                        "show": true,
                        "labelOrientation": "vertical"
                    }
                ],
                "categoryLabels": [
                    {
                        "show": true
                    }
                ],
                "categoryAxis": [
                    {
                        "showAxisTitle": true
                    }
                ],
                "valueAxis": [
                    {
                        "showAxisTitle": true
                    }
                ],
                "border": [
                    {
                        "show": false,
                        "radius": 0
                    }
                ],
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#E2E2E2"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        
        "card": {
            "*": {
                "labels": [
                    {
                        "fontSize": 27
                    }
                ],
                "categoryLabels": [
                    {
                        "fontSize": 12
                    }
                ]
            }
        },
        "clusteredBarChart": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "Comments go here for legend"
				}],
				"categoryAxis": [{
					"show": true,
					"position": "Right",
					"preferredCategoryWidth": 30,
					"maxMarginFactor": 28,
					"innerPadding": 22,
					"concatenateLabels": true,
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "Category axis title goes here"
				}],
				"valueAxis": [{
					"show": true,
					"position": "Right",
					"axisScale": "Linear",
					"start": 0,
					"end": 10,
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"gridlineShow": true,
					"gridlineThickness": 2,
					"gridlineStyle": "solid"
				}],
				"dataPoint": [{
				}],
				"labels": [{
					"show": true,
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "OutsideEnd",
					"enableBackground": true
				}],		
				"plotArea": [{
				}]
			}
		},
        "scatterChart": {
            "*": {
                "fillPoint": [
                    {
                        "show": true
                    }
                ]
            }
        },
        "lineChart": {
            "*": {
                "legend": [
                    {
                        "legendMarkerRendering": "lineAndMarker"
                    }
                ],
                "lineStyles": [
                    {
                        "showMarker": true,
                        "showSeries": true
                    }
                ]
            }
        },
        "pieChart": {
            "*": {
                "legend": [
                    {
                        "show": true
                    }
                ],
                "labels": [
                    {
                        "labelStyle": "Percent of total"
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#e2e2e2"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        }
    }
}