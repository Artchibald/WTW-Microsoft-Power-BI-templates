{
    "name": "WTW-theme3",
    "dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc",
        "#ffa3c2",
        "#ffbfac",
        "#007d61",
        "#b1232d"
    ],
    "firstLevelElements": "#000000",
    "secondLevelElements": "#000000",
    "thirdLevelElements": "#000000",
    "fourthLevelElements": "#000000",
    "background": "#FFFFFF",
    "foreground": "#3adcc9",
    "secondaryBackground": "#e2e2e2",
    "tableAccent": "#327fef",
    "good": "#007d61",
    "neutral": "#ffb92a", 
    "bad": "#b1232d",
    "maximum": "#327fef",
    "center": "#48086f",
    "minimum": "#c900ac",
    "null": "#f6517f",
    "textClasses": {
        "callout": {
            "fontSize": 45, 
            "fontFace": "Arial",
            "color": "#000000"
        },
        "title": {
            "fontSize": 24,
            "fontFace": "Arial",
            "color": "#000000"
        },
        "header": {
            "fontSize": 12,
            "fontFace": "Arial", 
            "color": "#000000"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Arial",
            "color": "#000000" 
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "responsive": true,
                        "wordWrap": true,
                        "fontSize": 11
                    }
                ],
                "wordWrap": [
                    {
                        "show": true
                    }
                ],
                "labels": [
                    {
                        "show": true,
                        "labelOrientation": "vertical"
                    }
                ],
                "categoryLabels": [
                    {
                        "show": true
                    }
                ],
                "categoryAxis": [
                    {
                        "showAxisTitle": true
                    }
                ],
                "valueAxis": [
                    {
                        "showAxisTitle": true
                    }
                ],
                "border": [
                    {
                        "show": true,
                        "radius": 2
                    }
                ],
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#E2E2E2"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        
        "card": {
            "*": {
                "labels": [
                    {
                        "fontSize": 27
                    }
                ],
                "categoryLabels": [
                    {
                        "fontSize": 12
                    }
                ]
            }
        },
        "scatterChart": {
            "*": {
                "fillPoint": [
                    {
                        "show": true
                    }
                ]
            }
        },
        "lineChart": {
            "*": {
                "legend": [
                    {
                        "legendMarkerRendering": "lineAndMarker"
                    }
                ],
                "lineStyles": [
                    {
                        "showMarker": true,
                        "showSeries": true
                    }
                ]
            }
        },
        "pieChart": {
            "*": {
                "legend": [
                    {
                        "show": true
                    }
                ],
                "labels": [
                    {
                        "labelStyle": "Percent of total"
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 100
                    }
                ]
            }
        }
    }
}