{
    "name": "WTW-theme3",
    "dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc",
        "#ffa3c2",
        "#ffbfac",
        "#007d61",
        "#b1232d"
    ],
    "firstLevelElements": "#2f2c31",
    "secondLevelElements": "#2f2c31",
    "thirdLevelElements": "#2f2c31",
    "fourthLevelElements": "#2f2c31",
    "background": "#FFFFFF",
    "foreground": "#3adcc9",
    "secondaryBackground": "#e2e2e2", 
    "tableAccent": "#327fef",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
    "maximum": "#327fef",
    "center": "#48086f",
    "minimum": "#c900ac",
    "null": "#f6517f",
    "textClasses": {
        "callout": {
            "fontSize": 40,
            "fontFace": "Segoe UI",
            "color": "#2f2c31"
        },
        "title": {
            "fontSize": 36,
            "fontFace": "Segoe UI",
            "color": "#2f2c31"
        },
        "header": {
            "fontSize": 14,
            "fontFace": "Segoe UI",
            "color": "#2f2c31"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Segoe UI",
            "color": "#2f2c31"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "responsive": true,
                        "wordWrap": true,
                        "fontSize": 11
                    }
                ],
                "wordWrap": [
                    {
                        "show": true
                    }
                ],
                "labels": [
                    {
                        "show": true,
                        "labelOrientation": "vertical"
                    }
                ],
                "categoryLabels": [
                    {
                        "show": true
                    }
                ],
                "categoryAxis": [
                    {
                        "showAxisTitle": true
                    }
                ],
                "valueAxis": [
                    {
                        "showAxisTitle": true
                    }
                ],
                "border": [
                    {
                        "show": false,
                        "radius": 0
                    }
                ],
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#E2E2E2"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        "actionButton": {
            "*": {
                "general": [
                    {
                        "responsive": true,
                        "keepLayerOrder": true
                    }
                ],
                "background": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "border": [
                    {
                        "color": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        },
                        "show": true,
                        "radius": 30
                    }
                ],
                "title": [
                    {
                        "fontSize": 21,
                        "weight": 30,
                        "show": true,
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "background": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        },
                        "alignment": "center"
                    }
                ],
                "visualHeader": [
                    {
                        "show": true
                    }
                ],
                "outline": [
                    {
                        "show": true
                    },
                    {
                        "$id": "default",
                        "weight": 5
                    },
                    {
                        "$id": "hover",
                        "weight": 5
                    },
                    {
                        "$id": "selected",
                        "weight": 5
                    },
                    {
                        "$id": "disabled",
                        "weight": 5
                    }
                ],
                "fill": [
                    {
                        "show": true
                    },
                    {
                        "$id": "default",
                        "transparency": 0,
                        "fillColor": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        }
                    },
                    {
                        "$id": "hover",
                        "transparency": 0,
                        "fillColor": {
                            "solid": {
                                "color": "#bc20a6"
                            }
                        }
                    },
                    {
                        "$id": "selected",
                        "transparency": 0,
                        "fillColor": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        }
                    },
                    {
                        "$id": "disabled",
                        "transparency": 0,
                        "fillColor": {
                            "solid": {
                                "color": "#A71D93"
                            }
                        }
                    }
                ],
                "icon": [
                    {
                        "show": true
                    },
                    {
                        "$id": "default",
                        "lineColor": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        }
                    },
                    {
                        "$id": "hover",
                        "lineColor": {
                            "solid": {
                                "color": "#bc20a6"
                            }
                        }
                    },
                    {
                        "$id": "selected",
                        "lineColor": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        }
                    },
                    {
                        "$id": "disabled",
                        "lineColor": {
                            "solid": {
                                "color": "#A71D93"
                            }
                        }
                    }
                ],
                "text": [
                    {
                        "show": true
                    },
                    {
                        "$id": "default",
                        "fontColor": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        },
                        "horizontalAlignment": "center"
                    },
                    {
                        "$id": "hover",
                        "fontColor": {
                            "solid": {
                                "color": "#bc20a6"
                            }
                        },
                        "horizontalAlignment": "center"
                    },
                    {
                        "$id": "selected",
                        "fontColor": {
                            "solid": {
                                "color": "#d124b8"
                            }
                        },
                        "horizontalAlignment": "center"
                    },
                    {
                        "$id": "disabled",
                        "fontColor": {
                            "solid": {
                                "color": "#A71D93"
                            }
                        },
                        "horizontalAlignment": "center"
                    }
                ]
            }
        },
        "areaChart": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "WTW legend",
					"legendMarkerRendering": "markerOnly",
					"matchLineColor": false
				}],
				"categoryAxis": [{
					"show": true,
					"axisType": "categorical",
					"concatenateLabels": true,
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "WTW Cat Ax"
				}],
				"valueAxis": [{
					"show": true,
					"position": "Right",
					"axisScale": "Linear",
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "WTW Val Ax",
					"gridlineShow": true,
					"gridlineThickness": 2,
					"gridlineStyle": "solid"
				}],
				"dataPoint": [{
				}],
				"labels": [{
					"show": true
				}],	
				"lineStyles": [{
					"strokeWidth": 4,
					"strokeLineJoin": "bevel",
					"lineStyle": "solid",
					"showMarker": true,
					"markerShape": "square",
					"markerSize": 10
				}],
				"plotArea": [{
				}]
			}
		},
        "azureMap": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true 
				}],
				"background": [{
					"show": true
				}],
				"border": [{
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"alignment": "center"
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "WTW Azure Map info goes here"
				}]
            }
        },
        "card": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"labels": [{
					"labelDisplayUnits": "0",
					"labelPrecision": 0
				}],
				"categoryLabels": [{
					"show": true
				}],
				"wordWrap": [{
					"show": true
				}],
				"background": [{
						"show": true,
						"transparency": 0
				}],
				  "title": [{
				    "show": true,
				    "alignment": "left"
				  }],
				  "border": [{
				    "show": true,
					"radius": 0
				  }]
			}
		},
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#e2e2e2"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        }
    }
}