{
	"name": "HundredPercentStackedColumnTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"hundredPercentStackedColumnChart": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "Hundred percent stacked column legend"
					}],
				"categoryAxis": [{
					"show": true,
					"position": "Right",
					"concatenateLabels": true,
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "Hundred percent stacked column category axis"
					}],
				"valueAxis": [{
					"show": true,
					"position": "Right",
					"axisScale": "Linear",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "Hundred percent stacked column value axis",
					"gridlineShow": true,
					"gridlineThickness": 2,
					"gridlineStyle": "solid"
				}],
				"dataPoint": [{
				}],
				"labels": [{
					"show": false,
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelOrientation": "vertical",
					"labelPosition": "OutsideEnd",
					"enableBackground": false,
					"backgroundTransparency": 0
				}],		
				"plotArea": [{
					"transparency": 0
				}]
			}
		}
	}
}