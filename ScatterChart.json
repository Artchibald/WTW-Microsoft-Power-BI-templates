{
	"name": "ScatterChartTemplate",
	"visualStyles": {
		"scatterChart": {
			"*": {
				"general": [{
					"dataVolume": 10000,
					"responsive": true,
					"keepLayerOrder": true
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "",
					"labelColor": { "solid": { "color": "#3adcc9"}},
					"fontFamily": "Courier New",
					"fontSize": 10
				}],
				"categoryAxis": [{
					"show": true,
					"axisScale": "Linear",
					"start": 0,
					"end": 10,
					"labelColor": { "solid": { "color": "#3adcc9"}},
					"fontSize": 14,
					"fontFamily": "Courier New",
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"color": { "solid": { "color": "3adcc9"}},
					"titleText": "asdf",
					"titleFontSize": 12,
					"titleFontFamily": "Courier New",
					"gridlineShow": true,
					"gridlineColor": { "solid": { "color": "#3adcc9"}},
					"gridlineThickness": 2,
					"gridlineStyle": "dashed"
				}],
				"valueAxis": [{
					"show": true,
					"position": "Right",
					"axisScale": "Linear",
					"start": 0,
					"end": 10,
					"labelColor": { "solid": { "color": "#3adcc9"}},
					"fontSize": 14,
					"fontFamily": "Courier New",
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"color": { "solid": { "color": "3adcc9"}},
					"titleText": "asdf",
					"titleFontSize": 12,
					"titleFontFamily": "Courier New",
					"gridlineShow": true,
					"gridlineColor": { "solid": { "color": "#3adcc9"}},
					"gridlineThickness": 2,
					"gridlineStyle": "dashed"
				}],
				"dataPoint": [{
					"defaultColor": { "solid": { "color": "#448822"}}
				}],
				"shapes": [{
					"markerSize": 3,
					"markerStyle": "triangle",
					"customizeSeries": true
				}],
				"plotArea": [{
					"transparency": 20
				}],
				"categoryLabels": [{
					"show": true,
					"color": { "solid": { "color": "#EEEEEE"}},
					"fontSize": 11,
					"fontFamily": "Courier New",
					"enableBackground": true,
					"backgroundColor": { "solid": { "color": "#0928DD"}},
					"backgroundTransparency": 30
				}],	
				"fillPoint": [{
					"show": false
				}],
				"colorByCategory": [{
					"show": false
				}]
			}
		}
	}
}