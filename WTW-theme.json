{
    "name": "WTW-theme",
    "dataColors": [
        "#48086f",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
    "textClasses": {
        "___COMMENT": "All text stuff",
        "callout": {
            "fontSize": 45,
            "fontFace": "Graphik, Arial",
            "color": "#252423"
        },
        "title": {
            "fontSize": 18,
            "fontFace": "Graphik, Arial",
            "color": "#252423"
        },
        "header": {
            "fontSize": 12,
            "fontFace": "Graphik, Arial",
            "color": "#252423"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Graphik, Arial",
            "color": "#252423"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [{
                    "responsive": true,
                    "wordWrap": true,
                    "fontSize": 11
                }],
                "wordWrap": [{
                    "show": true
                }],
                "labels": [{
                    "show": true,
                    "labelOrientation": "vertical"
                }],
                "categoryLabels": [{
                    "show": true
                }],
                "categoryAxis": [{
                    "showAxisTitle": true
                }],
                "valueAxis": [{
                    "showAxisTitle": true
                }]
            }
        },
        "card": {
            "*": {
                "labels": [{
                    "fontSize": 27
                }],
                "categoryLabels": [{
                    "fontSize": 12
                }]
            }
        },
        "scatterChart": {
            "*": {
                "fillPoint": [{
                    "show": true
                }]
            }
        },
        "lineChart": {
            "*": {
                "legend": [{
                    "legendMarkerRendering": "lineAndMarker"
                }],
                "lineStyles": [{
                    "showMarker": true,
                    "showSeries": true
                }]
            }
        },
        "donutChart": {
            "*": {
                "legend": [{
                    "show": true
                }],
                "labels": [{
                    "labelStyle": "Percent of total"
                }]
            }
        },
        "pieChart": {
            "*": {
                "legend": [{
                    "show": true
                }],
                "labels": [{
                    "labelStyle": "Percent of total"
                }]
            }
        }
    }
}