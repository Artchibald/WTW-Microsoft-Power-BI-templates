{
	"name": "HundredPercentStackedBarTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"hundredPercentStackedBarChart": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "WTW 100% Stacked Bar Chart Legend"
				}],
				"categoryAxis": [{
					"show": true,
					"position": "Right",
					"preferredCategoryWidth": 30,
					"maxMarginFactor": 28,
					"innerPadding": 22,
					"concatenateLabels": true,
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "WTW 100% Stacked Bar Chart Legend"
				}],
				"valueAxis": [{
					"show": true,
					"position": "Right",
					"axisScale": "Linear",
					"start": 0,
					"end": 10,
                    "labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "WTW 100% Stacked Bar Chart Value Axis",
					"gridlineShow": true,
					"gridlineThickness": 2,
					"gridlineStyle": "solid"
				}],
				"dataPoint": [{
				}],
				"labels": [{
					"show": true,
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelOrientation": "vertical",
					"labelPosition": "OutsideEnd",
					"enableBackground": true,
					"backgroundTransparency": 0
				}],		
				"plotArea": [{
					"transparency": 0
				}]
			}
		}
	}
}