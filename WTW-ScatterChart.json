{
	"name": "ScatterChartTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
	"visualStyles": {
		"scatterChart": {
			"*": {
				"general": [{
					"dataVolume": 10000,
					"responsive": true,
					"keepLayerOrder": true
				}],
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "WTW scater chart legend"
				}],
				"categoryAxis": [{
					"show": true,
					"axisScale": "Linear",
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "WTW scater chart cat axis",
					"gridlineShow": true,
					"gridlineThickness": 2,
					"gridlineStyle": "solid"
				}],
				"valueAxis": [{
					"show": true,
					"position": "Right",
					"axisScale": "Linear",
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"axisStyle": "showTitleOnly",
					"titleText": "WTW scater chart value axis",
					"gridlineShow": true,
					"gridlineThickness": 2,
					"gridlineStyle": "solid"
				}],
				"shapes": [{
					"markerSize": 3,
					"markerStyle": "square",
					"customizeSeries": true
				}],
				"categoryLabels": [{
					"show": true,
					"enableBackground": false
				}],	
				"fillPoint": [{
					"show": false
				}],
				"colorByCategory": [{
					"show": false
				}]
			}
		}
	}
}