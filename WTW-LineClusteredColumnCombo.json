{
    "name": "LineClusteredColumnComboTemplate",
    "dataColors": [
        "#d124b8",
        "#c900ac",
        "#f6517f",
        "#ff8204",
        "#ffb92a",
        "#3adcc9",
        "#327fef",
        "#808080",
        "#c2a8f0",
        "#e377dc"
    ],
    "background": "#FFFFFF",
    "foreground": "#e2e2e2",
    "tableAccent": "#fe6db6",
    "good": "#007d61",
    "neutral": "#ffb92a",
    "bad": "#b1232d",
    "visualStyles": {
        "lineClusteredColumnComboChart": {
            "*": {
                "general": [
                    {
                        "responsive": true,
                        "keepLayerOrder": true
                    }
                ],
                "legend": [
                    {
                        "show": true,
                        "position": "Top",
                        "showTitle": true,
                        "titleText": "WTW line clustered legend"
                    }
                ],
                "categoryAxis": [
                    {
                        "show": true,
                        "position": "Right",
                        "concatenateLabels": true,
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW line clustered category axis"
                    }
                ],
                "valueAxis": [
                    {
                        "show": true,
                        "position": "Right",
                        "axisScale": "Linear",
                        "labelDisplayUnits": "0",
                        "labelPrecision": "Auto",
                        "showAxisTitle": true,
                        "axisStyle": "showTitleOnly",
                        "titleText": "WTW line clustered value axis",
                        "gridlineShow": true,
                        "gridlineStyle": "solid",
                        "secShow": true,
                        "alignZeros": true,
                        "secAxisLabel": "WTW line clustered sec axis",
                        "secPosition": "Left",
                        "secAxisScale": "Log",
                        "secShowAxisTitle": true,
                        "secAxisStyle": "showTitleOnly",
                        "secTitleText": "WTW line clustered sec value axis"
                    }
                ],
                "labels": [
                    {
                        "show": false,
                        "labelOrientation": "vertical",
                        "labelPosition": "OutsideEnd",
                        "enableBackground": true
                    }
                ],
                "lineStyles": [
                    {
                        "shadeArea": true,
                        "strokeWidth": 4,
                        "strokeLineJoin": "bevel",
                        "showMarker": true,
                        "markerShape": "square",
                        "markerSize": 3
                    }
                ]
            }
        }
    }
}