{
    "name": "ClusteredBarTemplate",
    "visualStyles": {
        "clusteredBarChart": {
            "*": {
                "general": [{
                    "responsive": true,
                    "keepLayerOrder": true
                }],
                "legend": [{
                    "show": true,
                    "position": "Top",
                    "showTitle": true,
                    "titleText": "",
                    "labelColor": { "solid": { "color": "#7f35b2" } },
                    "fontFamily": "Graphik, Arial",
                    "fontSize": 10
                }],
                "categoryAxis": [{
                    "show": true,
                    "position": "Right",
                    "labelColor": { "solid": { "color": "#7f35b2" } },
                    "fontSize": 14,
                    "fontFamily": "Graphik, Arial",
                    "preferredCategoryWidth": 30,
                    "maxMarginFactor": 28,
                    "innerPadding": 22,
                    "concatenateLabels": true,
                    "showAxisTitle": true,
                    "axisStyle": "showTitleOnly",
                    "titleColor": { "solid": { "color": "#7f35b2" } },
                    "titleText": "asdf",
                    "titleFontSize": 12,
                    "titleFontFamily": "Graphik, Arial"
                }],
                "valueAxis": [{
                    "show": true,
                    "position": "Right",
                    "axisScale": "Linear",
                    "start": 0,
                    "end": 10,
                    "labelColor": { "solid": { "color": "#7f35b2" } },
                    "fontSize": 14,
                    "fontFamily": "Graphik, Arial",
                    "labelDisplayUnits": "0",
                    "labelPrecision": "Auto",
                    "showAxisTitle": true,
                    "axisStyle": "showTitleOnly",
                    "color": { "solid": { "color": "7f35b2" } },
                    "titleText": "asdf",
                    "titleFontSize": 12,
                    "titleFontFamily": "Graphik, Arial",
                    "gridlineShow": true,
                    "gridlineColor": { "solid": { "color": "#7f35b2" } },
                    "gridlineThickness": 2,
                    "gridlineStyle": "dashed"
                }],
                "dataPoint": [{
                    "defaultColor": { "solid": { "color": "#7f35b2" } }
                }],
                "labels": [{
                    "show": true,
                    "color": { "solid": { "color": "#7f35b2" } },
                    "labelDisplayUnits": "0",
                    "labelPrecision": 1,
                    "labelPosition": "OutsideEnd",
                    "fontSize": 11,
                    "fontFamily": "Graphik, Arial",
                    "enableBackground": true,
                    "backgroundColor": { "solid": { "color": "#7f35b2" } },
                    "backgroundTransparency": 30
                }],
                "plotArea": [{
                    "transparency": 20
                }]
            }
        }
    }
}